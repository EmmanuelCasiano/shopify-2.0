<footer class="treah-footer">
    <div>
        {% for link in section.settings.footer_menu.links %}
            <a href="{{ link.url }}">{{ link.title }}</a>
        {% endfor %}
    </div>
</footer>


{% schema %}
{
    "name": "Treah Footer",
    "settings": [
        {
            "type": "link_list",
            "id": "footer_menu",
            "label": "Footer Menu"
        }
    ],
    "presets": [
        {
            "name": "Treah Footer"
        }
    ]

}
{% endschema %}